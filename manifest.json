{
  "name": "FP Affiliate Extractor",
  "key": "",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Pushes user's First Promoter data to Google Sheet",
  "icons": {
    "16": "icon/icon16x16.png",
    "48": "icon/icon48x48.png",
    "128": "icon/icon128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon19x19.png",
      "38": "icon/icon38x38.png"
    },
    "default_popup": "popup.html"
  },
  "background" : {
    "scripts" : ["jquery-3.2.1.min.js","background.js"],
    "persistent": true
  },
  "oauth2": {
    "client_id": "289844448623-f85hrjoc2l48qc53jcv60ue7vfi491nh.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "permissions": [
    "storage",
    "identity",
    "<all_urls>",
    "http://office.legendarymarketer.com/*",
    "https://office.legendarymarketer.com/*"
  ]
}
